{
  "name": "lukasss93/laravel-route-debug",
  "description": "Print the route name and action in the response headers.",
  "keywords": [
    "laravel",
    "route",
    "debug",
    "name",
    "action"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Luca Patera",
      "email": "lucapatera@outlook.it",
      "homepage": "https://www.lucapatera.it/",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/contracts": "^10.0|^11.0|^12.0",
    "illuminate/http": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "pestphp/pest": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Lukasss93\\Laravel\\RouteDebug\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lukasss93\\Laravel\\RouteDebug\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lukasss93\\Laravel\\RouteDebug\\RouteDebugServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "./vendor/bin/pest --colors=always"
  }
}
